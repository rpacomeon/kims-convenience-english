    },
    {
      "id": "ARTICLE_THE_OVERUSE_1",
      "pattern": "\\ball the gay\\b",
      "correction": "all gay",
      "explanation": "형용사 앞에는 보통 'the'를 사용하지 않습니다",
      "example_wrong": "They is all the gay",
      "example_correct": "They are all gay"
    },
    {
      "id": "ARTICLE_THE_OVERUSE_2",
      "pattern": "\\bthe most best\\b",
      "correction": "the best",
